{

  "description": "Allows you to save current tabs to be opened together later.",
  "manifest_version": 2,
  "name": "Filing Tabinent",
  "version": "1.0",
  "homepage_url": "https://github.com/tcphw5/FilingTabinent",
  "icons": {
    "48": "icons/cabinent.png"
  },

  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],

  "browser_action": {
    "default_icon": "icons/cabinent.png",
    "theme_icons": [{
        "light": "icons/cabinent.png",
        "dark": "icons/cabinent.png",
        "size": 64
    }],
    "default_title": "Filing Tabinent",
    "default_popup": "popup/save_tab.html"
  },

  "web_accessible_resources": [
  ]

}
